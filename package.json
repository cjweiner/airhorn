{
  "name": "airhorn",
  "productName": "Airhorn",
  "version": "0.1.1",
  "description": "It's an air horn.",
  "license": "MIT",
  "repository": "cjweiner/airhorn",
  "author": {
    "name": "Corey Weiner",
    "email": "cjweiner@gmail.com",
    "url": "corey-weiner.com"
  },
  "main": "./main.js",
  "scripts": {
    "start": "electron .",
    "build-mac": "electron-packager . --out=dist --asar --overwrite --platform=darwin --arch=x64 --prune=true --icon='./app/img/app-icon.icns'",
    "build-win": "electron-packager . --out=dist --overwrite --asar --platform=win32 --arch=ia32 --prune=true --icon=./app/img/app-icon.ico",
    "build-linux": "electron-packager . --out=dist --overwrite --asar --platform=linux --arch=x64 --prune=true --icon=./app/img/app-icon.png",
    "build-all": "npm run build-win && npm run build-mac && npm run build-linux"
  },
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "electron-debug": "^1.0.0",
    "nconf": "^0.8.4"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron": "1.7.8",
    "electron-packager": "^8.0.0"
  }
}
